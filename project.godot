; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Limage",
"language": "GDScript",
"path": "res://addons/limage/Limage.gd"
}, {
"base": "Node",
"class": "LimageButton",
"language": "GDScript",
"path": "res://addons/limage/LimageButton.gd"
}, {
"base": "Node",
"class": "LimageNode",
"language": "GDScript",
"path": "res://addons/limage/LimageNode.gd"
}, {
"base": "Node",
"class": "LimageOption",
"language": "GDScript",
"path": "res://addons/limage/LimageOption.gd"
} ]
_global_script_class_icons={
"Limage": "",
"LimageButton": "",
"LimageNode": "",
"LimageOption": ""
}

[application]

config/name="Limage"
config/icon="res://icon.png"

[display]

window/size/width=800

[editor_plugins]

enabled=PoolStringArray( "limage" )

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
